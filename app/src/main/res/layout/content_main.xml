<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.adnan.myapplication.MainActivity"
    tools:showIn="@layout/activity_main">


    <Button
        android:id="@+id/a1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minHeight="58dp"
        android:minWidth="74dp"
        android:background="#088736"
        android:layout_marginTop="75dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="48dp"
        android:layout_marginBottom="8dp" />

    <Button
        android:id="@+id/a2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minHeight="58dp"
        android:minWidth="74dp"
        android:background="#088736"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_alignTop="@+id/a1"
        android:layout_toRightOf="@+id/a1"
        android:layout_toEndOf="@+id/a1"
        android:layout_marginBottom="8dp" />

    <Button
        android:id="@+id/a3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minHeight="58dp"
        android:minWidth="74dp"
        android:background="#088736"
        android:layout_alignTop="@+id/a2"
        android:layout_toRightOf="@+id/a2"
        android:layout_marginLeft="8dp"
        android:layout_toEndOf="@id/a2" />

    <Button
        android:id="@+id/a4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minHeight="58dp"
        android:minWidth="74dp"
        android:background="#088736"
        android:layout_below="@+id/a1"
        android:layout_alignLeft="@+id/a1"
        android:layout_alignStart="@+id/a1"
        android:layout_marginBottom="8dp" />

    <Button
        android:id="@+id/a5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minHeight="58dp"
        android:minWidth="74dp"
        android:background="#088736"
        android:layout_alignTop="@+id/a4"
        android:layout_alignLeft="@+id/a2"
        android:layout_alignStart="@+id/a2" />

    <Button
        android:id="@+id/a6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minHeight="58dp"
        android:minWidth="74dp"
        android:background="#088736"
        android:layout_alignTop="@+id/a5"
        android:layout_toRightOf="@+id/a5"
        android:layout_toEndOf="@+id/a5"
        android:layout_marginLeft="8dp" />

    <Button
        android:id="@+id/a7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minHeight="58dp"
        android:minWidth="74dp"
        android:background="#088736"
        android:layout_below="@+id/a5"
        android:layout_alignLeft="@+id/a4"
        android:layout_alignStart="@+id/a4"
        android:layout_marginTop="8dp" />

    <Button
        android:id="@+id/a8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minHeight="58dp"
        android:minWidth="74dp"
        android:background="#088736"
        android:layout_below="@+id/a5"
        android:layout_toRightOf="@+id/a7"
        android:layout_toEndOf="@+id/a7"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="8dp" />

    <Button
        android:id="@+id/a9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minHeight="58dp"
        android:minWidth="74dp"
        android:background="#088736"
        android:layout_below="@+id/a5"
        android:layout_toRightOf="@+id/a5"
        android:layout_toEndOf="@+id/a5"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp" />